  <strong>queries:</strong> <%= queries.count %> (<%= format_ms queries.sum(:duration) %>)
  <table id="queries" class="table">
    <% queries.each do |query| %>
      <tr>
        <td><%= format_ms query.duration %></td>
        <td><%= query.data['query'] %></td>
      </tr>
    <% end %>
  </table>
